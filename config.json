{
  "ffmpeg_path": "/opt/homebrew/bin/ffmpeg",
  "temp_dir": "/tmp/video_compression",
  "log_dir": "./logs",
  "compression_settings": {
    "target_bitrate_reduction": 0.5,
    "preserve_10bit": true,
    "preserve_metadata": true,
    "video_codec": "libx265",
    "preset": "medium",
    "crf": 23
  },
  "safety_settings": {
    "min_free_space_gb": 10,
    "verify_integrity": true,
    "create_backup_hash": true,
    "max_retries": 3
  }
}